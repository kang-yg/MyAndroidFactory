<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/clFragMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentCoordinatorLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="CoordinatorLayout" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentMotionLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="MotionLayout" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentNavigation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Navigation" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentWorkManager"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="WorkManager" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentCustomView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="CustomView" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentCompose"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Compose" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentPaging"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Paging" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentStorage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Storage" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentDataStore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="DataStore" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentHandlerThread"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="HandlerThread" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btGoFragmentCamera"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="CameraX" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>